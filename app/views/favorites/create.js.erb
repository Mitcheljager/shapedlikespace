document.querySelectorAll("[data-favorite='<%= @model.id %>']").forEach(element => element.innerHTML = `<%= render 'form', favoritable_type: @model_name.to_s, favoritable_id: @model.id %>`)
document.querySelectorAll("[data-favorite='<%= @model.id %>']").forEach(element => element.querySelector(".favorite").classList.add("favorite--animating"))
